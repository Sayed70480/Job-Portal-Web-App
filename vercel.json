{
  "version": 2,
  {
      "src": "Clients/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "Clients/build" }
    },
    {
      "src": "Server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Clients/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "Server/app.js"
    }
  ]
}
