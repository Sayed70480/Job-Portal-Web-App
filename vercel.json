{
  "version": 2,
"builds": [
    {
      "src": "Clients/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "Clients/build" }
    },
    {
      "src": "Server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Clients/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/Server/app.js"
    }
  ]
}
